          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="KafkaConsumer">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">@Grab(group=&apos;org.apache.kafka&apos;, module=&apos;kafka-clients&apos;, version=&apos;3.5.1&apos;)
import org.apache.kafka.clients.consumer.KafkaConsumer
import java.util.Properties

def props = new Properties()
props.put(&apos;bootstrap.servers&apos;, &apos;localhost:9092&apos;)
props.put(&apos;key.deserializer&apos;, &apos;org.apache.kafka.common.serialization.StringDeserializer&apos;)
props.put(&apos;value.deserializer&apos;, &apos;org.apache.kafka.common.serialization.StringDeserializer&apos;)
props.put(&apos;group.id&apos;, &apos;jmeter-test-group&apos;)

def consumer = new KafkaConsumer&lt;String, String&gt;(props)
consumer.subscribe([&apos;uber&apos;])  // Replace with your Kafka topic

def messages = []
def records = consumer.poll(1000)
for (record in records) {
    messages.add(record.value())
}

log.info(&quot;Kafka Messages: &quot; + messages)
vars.put(&quot;kafkaMessages&quot;, messages.toString())
